{
  "name": "scroll-down-icon",
  "version": "1.0.5",
  "private": false,
  "description": "An animated icon stimulating scrolling",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.9.5",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^22.2.3",
    "jest": "^23.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitflower/scroll-down-icon.git"
  },
  "author": "Matthias Max",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitflower/scroll-down-icon"
  },
  "homepage": "https://www.bitflower.net",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
